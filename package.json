{
  "name": "api-def",
  "version": "0.5.0-alpha1",
  "description": "Typed API definitions with middleware support",
  "main": "cjs/index.js",
  "types": "esm/index.d.ts",
  "module": "esm/index.js",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "test": "npm run test:types && npm run test:lint && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:lint": "npm run lint:strict",
    "test:types": "tsc --noEmit -p .",
    "example:start": "cd example && npm run start",
    "lint": "eslint --ext .tsx,.ts src",
    "lint:fix": "eslint --fix --ext .tsx,.ts src",
    "lint:strict": "npm run lint -- --max-warnings 0",
    "cleanup": "rimraf esm && rimraf cjs",
    "build": "npm run cleanup && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es2015 --target es5 --outDir esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir cjs",
    "build:watch": "npm-run-all -p \"build:esm -- -w\" \"build:cjs -- -w\"",
    "website:dev": "cd website && npm run start",
    "website:deploy": "cd website && npm run deploy"
  },
  "keywords": [
    "typescript",
    "javascript",
    "node",
    "web",
    "api",
    "typed",
    "cache",
    "fetch",
    "retry",
    "middleware"
  ],
  "author": "James Waterhouse <09jwater@gmail.com>",
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "esm/",
    "cjs/"
  ],
  "repository": "https://github.com/Censkh/api-def",
  "devDependencies": {
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@types/axios": "0.14.0",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.6",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "cross-env": "7.0.3",
    "eslint": "7.31.0",
    "jest": "27.0.6",
    "npm-run-all": "4.1.5",
    "typescript": "4.3.5"
  }
}
